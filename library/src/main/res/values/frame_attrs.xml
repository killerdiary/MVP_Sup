<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="rivRadius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <attr name="rbRatioWidth" format="integer"/>
        <attr name="rbStarNormal" format="reference"/>
        <attr name="rbStarSelected" format="reference"/>
        <attr name="rbStarWidth" format="integer"/>
        <attr name="rbStarHeight" format="integer"/>
        <attr name="rbProgress" format="float"/>
        <attr name="rbMaxProgress" format="integer"/>
        <attr name="rbChildSpace" format="integer"/>
        <attr name="rbStep" format="float"/>
        <attr name="rbAutoMeasure" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="pbNormal" format="reference"/>
        <attr name="pbSelected" format="reference"/>
        <attr name="pbProgress" format="float"/>
        <attr name="pbMaxProgress" format="integer"/>
        <attr name="pbStep" format="float"/>
    </declare-styleable>
</resources>